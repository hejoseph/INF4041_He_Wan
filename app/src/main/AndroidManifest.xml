<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.esiea.he_wan.mybasicapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <service android:name=".DownloadJson" />

        <receiver
            android:name=".ReceiveBroadcast"
            android:enabled="true"
            android:exported="true">
            <intent-filter>
                <action android:name="com.esiea.he_wan.sendbroadcast" />
            </intent-filter>
        </receiver>

        <activity
            android:name=".RVJsonFile"
            android:label="@string/title_activity_rvjson_file"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>
